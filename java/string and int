public class HelloWorld {
    public static void main(String[] args) {
        String kanishk;
        kanishk = "Kanishk Batra";
        System.out.println(kanishk);

        int salaryWeekly =3200;
        int salaryMonthly = salaryWeekly*4;
        System.out.println(salaryWeekly);
        System.out.println(salaryMonthly);
        
        int apples=5;
        int oranges=6;
        int fruit = apples+oranges;
        System.out.println(fruit);

    }
}
